{
  "name": "frontend-base",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "storybook dev -p 6006",
    "start:webpack": "webpack serve",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run build:storybook && npm run build:frontend &&  npm run build:coverage",
    "build:storybook": "storybook build && rm -rf ./Frontend && mkdir ./Frontend && mv storybook-static/* ./Frontend",
    "build:frontend": "webpack --mode production && mv dist/remoteEntry.js ./Frontend",
    "build:coverage": "jest --coverage && mv coverage/lcov-report ./Frontend/coverage-report",
    "serve:storybook": "serve storybook-static",
    "serve": "serve Frontend",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-themes": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.2.4",
    "react-test-renderer": "^18.2.0",
    "serve": "^14.2.1",
    "storybook": "^7.6.10",
    "storybook-dark-mode": "^3.0.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fontsource/material-icons": "^5.0.11",
    "@fontsource/roboto": "^5.0.8",
    "@mui/material": "^5.15.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
